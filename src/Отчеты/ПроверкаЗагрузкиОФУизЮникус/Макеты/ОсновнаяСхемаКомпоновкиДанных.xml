				<dataSource>ИсточникДанных1</dataSource>
				<query>ВЫБРАТЬ
	НАЧАЛОПЕРИОДА(Док.Дата, ДЕНЬ) КАК Дата,
	Док.ДоговорКонтрагента.Наименование КАК Договор,
	Док.ДЦ КАК ДЦ,
	Док.Сумма КАК Сумма,
	Док.СуммаКВ КАК СуммаКВ,
	Док.Ссылка КАК Документ,
	0 КАК СуммаЮникус,
	0 КАК СуммаКВЮникус
{ВЫБРАТЬ
	Дата,
	Договор,
	ДЦ.*,
	Документ.*,
	СуммаКВ,
	Сумма,
	СуммаКВЮникус,
	СуммаЮникус}
ИЗ
	Документ.АСЦ_ПлановоеНачислениеКВ КАК Док
ГДЕ
	Док.Дата МЕЖДУ &amp;Дата1 И &amp;Дата2
	И НЕ Док.ПометкаУдаления
	И Док.ВидОперации В (ЗНАЧЕНИЕ(Справочник.АСЦ_ВидыОперацийПлановоеНачислениеКВ.Страховки), ЗНАЧЕНИЕ(Справочник.АСЦ_ВидыОперацийПлановоеНачислениеКВ.ПрямыеРасчетыССК))
	И (Док.Сумма &lt;&gt; 0
			ИЛИ Док.СуммаКВ &lt;&gt; 0)
{ГДЕ
	Док.ДЦ.* КАК ДЦ,
	Док.Ссылка.* КАК Документ}</query>
