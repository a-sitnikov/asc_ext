				<dataSource>ИсточникДанных1</dataSource>
				<query>ВЫБРАТЬ
	Док.Ссылка КАК Документ,
	Док.Реализация КАК Реализация,
	Док.ДЦ КАК ДЦ,
	Док.Статья КАК Статья,
	ВЫБОР
		КОГДА Док.ОтчетПосредника = ЗНАЧЕНИЕ(Документ.АСЦ_ОтчетПосредникаУпр.ПустаяСсылка)
			ТОГДА Док.СуммаКВ
		ИНАЧЕ Док.Основание.СуммаКВ
	КОНЕЦ КАК СуммаКВ,
	ВЫБОР
		КОГДА Док.ОтчетПосредника = ЗНАЧЕНИЕ(Документ.АСЦ_ОтчетПосредникаУпр.ПустаяСсылка)
			ТОГДА 0
		ИНАЧЕ Док.СуммаКВ
	КОНЕЦ КАК СуммаКВФакт,
	0 КАК СуммаКВАстра,
	0 КАК СуммаКВФактАстра
{ВЫБРАТЬ
	Документ.*,
	Реализация.*,
	ДЦ.*,
	Статья.*,
	СуммаКВ,
	СуммаКВФакт,
	СуммаКВАстра,
	СуммаКВФактАстра}
ИЗ
	Документ.АСЦ_ПлановоеНачислениеКВ КАК Док
ГДЕ
	Док.Дата МЕЖДУ &amp;Дата1 И &amp;Дата2
	И Док.ВидОперации.НастройкаСоотвествияВыгрузкаАстра.Наименование = "КВ банков"
	И НЕ Док.ПометкаУдаления
{ГДЕ
	Док.Ссылка.* КАК Документ,
	Док.ДЦ.*,
	Док.Статья.*,
	Док.СуммаКВ,
	Док.Реализация.*}</query>
