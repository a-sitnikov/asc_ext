			ТОГДА Рег.Документ.Основание
	КОНЕЦ КАК ДокументРеализации,
	ВЫБОР
		КОГДА Рег.Документ.ЭтоКорректировка
			ТОГДА Рег.Документ.Основание.Дата
		ИНАЧЕ Рег.Период
	КОНЕЦ КАК Дата,
	Рег.Документ.Номенклатура КАК Номенклатура,
	Рег.Документ.Номенклатура.НоменклатурнаяГруппа КАК НоменклатурнаяГруппа,
	Рег.Документ.VIN КАК VIN,
	Рег.Комитент КАК Комитент,
	Рег.Документ.Контрагент КАК Контрагент,
	Рег.Документ.ДоговорКонтрагента КАК ДоговорКонтрагента,
	Рег.Организация КАК Организация,
	Рег.ДЦ КАК ДЦ,
	Рег.Статья КАК Статья,
	Рег.Сумма КАК Сумма,
	Рег.СуммаКВ КАК СуммаКВ,
	ВЫБОР
		КОГДА Рег.Документ.ЭтоКорректировка
				И Рег.Сумма = 0
			ТОГДА -1
		КОГДА Рег.Документ.Основание &lt;&gt; ЗНАЧЕНИЕ(Документ.АСЦ_ПлановоеНачислениеКВ.ПустаяСсылка)
				И Рег.Сумма &lt;&gt; 0
			ТОГДА 0
		ИНАЧЕ 1
	КОНЕЦ КАК КоличествоПродаж
{ВЫБРАТЬ
	Документ.*,
	Дата,
	Номенклатура.*,
	НоменклатурнаяГруппа.*,
	VIN,
	Комитент.*,
	Контрагент.*,
	Организация.*,
	ДЦ.*,
	Статья.*,
	Сумма,
	СуммаКВ,
	ДоговорКонтрагента.*,
	КоличествоПродаж}
ИЗ
	РегистрНакопления.АСЦ_НачисленноеКВ КАК Рег
ГДЕ
	ВЫБОР
			КОГДА Рег.Документ.ЭтоКорректировка
				ТОГДА Рег.Документ.Основание.Дата
			ИНАЧЕ Рег.Период
		КОНЕЦ МЕЖДУ &amp;Дата1 И &amp;Дата2
	И &amp;ПериодКорректировок = "Реализация"
{ГДЕ
	Рег.Регистратор.* КАК Документ,
	Рег.Документ.Номенклатура.* КАК Номенклатура,
	Рег.Документ.Номенклатура.НоменклатурнаяГруппа.* КАК НоменклатурнаяГруппа,
	Рег.Документ.VIN КАК VIN,
	Рег.Документ.Контрагент.* КАК Контрагент,
	Рег.Документ.ДоговорКонтрагента.* КАК ДоговорКонтрагента,
	Рег.Организация.*,
	Рег.ДЦ.*,
	Рег.Статья.*,
	Рег.Сумма,
	Рег.СуммаКВ,
	Рег.Комитент.*}

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Рег.Регистратор,
	ВЫБОР
		КОГДА Рег.Документ.ЭтоКорректировка
			ТОГДА Рег.Документ.Основание
	КОНЕЦ,
	Рег.Период,
	Рег.Документ.Номенклатура,
	Рег.Документ.Номенклатура.НоменклатурнаяГруппа,
	Рег.Документ.VIN,
	Рег.Комитент,
	Рег.Документ.Контрагент,
	Рег.Документ.ДоговорКонтрагента,
	Рег.Организация,
	Рег.ДЦ,
	Рег.Статья,
	Рег.Сумма,
	Рег.СуммаКВ,
	ВЫБОР
		КОГДА Рег.Документ.ЭтоКорректировка
				И Рег.Сумма = 0
			ТОГДА -1
		КОГДА Рег.Документ.Основание &lt;&gt; ЗНАЧЕНИЕ(Документ.АСЦ_ПлановоеНачислениеКВ.ПустаяСсылка)
				И Рег.Сумма &lt;&gt; 0
			ТОГДА 0
		ИНАЧЕ 1
	КОНЕЦ
ИЗ
	РегистрНакопления.АСЦ_НачисленноеКВ КАК Рег
ГДЕ
	Рег.Период МЕЖДУ &amp;Дата1 И &amp;Дата2
	И &amp;ПериодКорректировок = "Корректировка"
{ГДЕ
	Рег.Регистратор.* КАК Документ,
	Рег.Документ.Номенклатура.* КАК Номенклатура,
	Рег.Документ.Номенклатура.НоменклатурнаяГруппа.* КАК НоменклатурнаяГруппа,
	Рег.Документ.VIN КАК VIN,
	Рег.Документ.Контрагент.* КАК Контрагент,
	Рег.Документ.ДоговорКонтрагента.* КАК ДоговорКонтрагента,
	Рег.Организация.*,
	Рег.ДЦ.*,
	Рег.Статья.*,
	Рег.Сумма,
	Рег.СуммаКВ,
