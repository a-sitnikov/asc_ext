				<query>ВЫБРАТЬ
	"Реализация" КАК Тип,
	ДокТЧ.Ссылка КАК Документ,
	ДокТЧ.Ссылка КАК ДокументРеализации,
	ДокТЧ.Ссылка.Дата КАК Дата,
	ДокТЧ.Номенклатура КАК Номенклатура,
	ДокТЧ.Номенклатура.НоменклатурнаяГруппа КАК НоменклатурнаяГруппа,
	ВЫРАЗИТЬ(СвойствоVIN.Значение КАК СТРОКА(20)) КАК VIN,
	ДокТЧ.Сумма КАК СуммаКВ,
	ДокТЧ.Сумма КАК Сумма,
	ДокТЧ.Ссылка.Контрагент КАК Контрагент,
	ДокТЧ.Ссылка.ДоговорКонтрагента КАК ДоговорКонтрагента,
	ДокТЧ.Ссылка.Организация КАК Организация,
	ДокТЧ.Ссылка.ДоговорКонтрагента.ОсновнойЦФО КАК ДЦ,
	ДокТЧ.Ссылка.ДоговорКонтрагента.ОсновнаяСтатьяИсполнение КАК Статья
{ВЫБРАТЬ
	Тип,
	Документ.*,
	ДокументРеализации.*,
	Дата,
	Номенклатура.*,
	НоменклатурнаяГруппа.*,
	VIN,
	СуммаКВ,
	Сумма,
	Контрагент.*,
	ДоговорКонтрагента.*,
	Организация.*,
	ДЦ.*,
	Статья.*}
ИЗ
	Документ.РеализацияТоваровУслуг.Услуги КАК ДокТЧ
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ДополнительныеСведения КАК СвойствоVIN
		ПО (СвойствоVIN.Объект = ДокТЧ.Ссылка)
			И (СвойствоVIN.Свойство.Имя = "VIN")
ГДЕ
	ДокТЧ.Ссылка.Дата МЕЖДУ &amp;Дата1 И &amp;Дата2
	И ДокТЧ.Ссылка.Проведен
	И ДокТЧ.Ссылка.ДоговорКонтрагента.ОсновнаяСтатьяИсполнение В ИЕРАРХИИ(&amp;СтатьяДоходов)
	И ДокТЧ.Ссылка.ДокументОснование = НЕОПРЕДЕЛЕНО
{ГДЕ
	("Реализация") КАК Тип,
	ДокТЧ.Ссылка.* КАК Документ,
	ДокТЧ.Номенклатура.*,
	ДокТЧ.Номенклатура.НоменклатурнаяГруппа.* КАК НоменклатурнаяГруппа,
	(ВЫРАЗИТЬ(СвойствоVIN.Значение КАК СТРОКА(20))) КАК VIN,
	ДокТЧ.Сумма,
	ДокТЧ.Ссылка.Контрагент.* КАК Контрагент,
	ДокТЧ.Ссылка.ДоговорКонтрагента.* КАК ДоговорКонтрагента,
	ДокТЧ.Ссылка.Организация.* КАК Организация,
	ДокТЧ.Ссылка.ДоговорКонтрагента.ОсновнойЦФО.* КАК ДЦ,
	ДокТЧ.Ссылка.ДоговорКонтрагента.ОсновнаяСтатьяИсполнение.* КАК Статья}

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	"Реализация",
	ДокТЧ.Ссылка,
	ДокТЧ.Ссылка,
	ДокТЧ.Ссылка.Дата,
	ДокТЧ.Номенклатура,
	ДокТЧ.Номенклатура.НоменклатурнаяГруппа,
	ВЫРАЗИТЬ(СвойствоVIN.Значение КАК СТРОКА(20)),
	0,
	ДокТЧ.Сумма,
	ДокТЧ.Ссылка.Контрагент,
	ДокТЧ.Ссылка.ДоговорКонтрагента,
	ДокТЧ.Ссылка.Организация,
	ДокТЧ.Ссылка.ДоговорКонтрагента.ОсновнойЦФО,
	ДокТЧ.Ссылка.ДоговорКонтрагента.ОсновнаяСтатьяИсполнение
ИЗ
	Документ.РеализацияТоваровУслуг.АгентскиеУслуги КАК ДокТЧ
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ДополнительныеСведения КАК СвойствоVIN
		ПО (СвойствоVIN.Объект = ДокТЧ.Ссылка)
			И (СвойствоVIN.Свойство.Имя = "VIN")
ГДЕ
	ДокТЧ.Ссылка.Дата МЕЖДУ &amp;Дата1 И &amp;Дата2
	И ДокТЧ.Ссылка.Проведен
	И ДокТЧ.Ссылка.ДоговорКонтрагента.ОсновнаяСтатьяИсполнение В ИЕРАРХИИ(&amp;СтатьяДоходов)
	И ДокТЧ.Ссылка.ДокументОснование = НЕОПРЕДЕЛЕНО
{ГДЕ
	("Реализация") КАК Тип,
	ДокТЧ.Ссылка.* КАК Документ,
	ДокТЧ.Номенклатура.*,
	ДокТЧ.Номенклатура.НоменклатурнаяГруппа.* КАК НоменклатурнаяГруппа,
	(ВЫРАЗИТЬ(СвойствоVIN.Значение КАК СТРОКА(20))) КАК VIN,
	ДокТЧ.Сумма,
	ДокТЧ.Ссылка.Контрагент.* КАК Контрагент,
	ДокТЧ.Ссылка.ДоговорКонтрагента.* КАК ДоговорКонтрагента,
	ДокТЧ.Ссылка.Организация.* КАК Организация,
	ДокТЧ.Ссылка.ДоговорКонтрагента.ОсновнойЦФО.* КАК ДЦ,
	ДокТЧ.Ссылка.ДоговорКонтрагента.ОсновнаяСтатьяИсполнение.* КАК Статья}</query>
